{
    "alg": {
        "n_episodes": 100000,
        "n_eval": 10,
        "n_test": 2,
        "period": 1000
    },
    "env": {
        "max_steps": 5,
        "n_agents": 2,
        "name": "ipd",
        "r_multiplier": 3.0
    },
    "lio": {
        "asymmetric": false,
        "entropy_coeff": 0.1,
        "epsilon_div": 500,
        "epsilon_end": 0.01,
        "epsilon_start": 1.0,
        "gamma": 0.99,
        "idx_recipient": 1,
        "lr_actor": 0.001,
        "lr_reward": 0.001,
        "optimizer": "adam",
        "reg": "l1",
        "reg_coeff": 0.0
    },
    "main": {
        "dir_name": "ipd_lio",
        "max_to_keep": 100,
        "model_name": "model.ckpt",
        "save_period": 100000,
        "seed": 1,
        "summarize": false
    },
    "nn": {
        "n_h1": 16,
        "n_h2": 8,
        "n_hr1": 16,
        "n_hr2": 8
    }
}